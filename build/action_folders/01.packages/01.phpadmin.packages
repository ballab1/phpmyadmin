# phpadmin Packages

if [ "${CBF['os_id']}" = 'alpine' ]; then
    declare PKGS='curl php7-session php7-mysqli php7-mbstring php7-xml php7-gd php7-zlib php7-bz2 php7-zip php7-openssl php7-curl php7-opcache php7-json nginx php7-fpm'
    package.checkIfInstalled "$PKGS"
    apk add --no-cache $PKGS
fi 
